apz.prdsel.Products = {};
apz.prdsel.Products.sparams = {};
apz.app.onLoad_Products = function(params){
    debugger;
    apz.prdsel.Products.sparams = params;
    $("body").addClass("loginbgg");
}
apz.prdsel.Products.fnShowSubProducts = function(products){
    debugger;
     apz.newApplication =true;
     apz.prdsel.ProductLauncher.fnNavigate("SubProducts",{
         "product":products,
         "parentdiv":apz.prdsel.Products.sparams.parentdiv
     });
}
