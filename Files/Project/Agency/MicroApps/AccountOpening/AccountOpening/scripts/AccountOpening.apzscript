apz.ACOP01.AccountOpening.fnprocesResponse = function(ljson) {
    debugger;
    try {
        var lxmlData = ljson;
        var lxmlDoc = $.parseXML(lxmlData);
        var $lxml = $(lxmlDoc);
        var $ltxtELem = $lxml.find("PrintLetterBarcodeData");
        var lcusjson = {};
        apz.ACOP01.AccountOpening.sParams.CustomerDetails = {};
        apz.ACOP01.AccountOpening.sParams.CustomerDetails.aadhar = $ltxtELem.attr("uid");
        apz.ACOP01.AccountOpening.sParams.CustomerDetails.firstName = $ltxtELem.attr("name");
        if (($ltxtELem.attr("gender").trim() == "Male") || ($ltxtELem.attr("gender").trim() == "MALE") || ($ltxtELem.attr("gender").trim() == "M") ||
            ($ltxtELem.attr("gender").trim() == "male")) {
            apz.ACOP01.AccountOpening.sParams.CustomerDetails.sex = "M";
        } else {
            apz.ACOP01.AccountOpening.sParams.CustomerDetails.sex = "F";
        }
        //apz.setElmValue("ACOP01__AadharNo__Gender", apz.ACOP01.aadharno.sParams.CustomerDetails.sex)
        if (!apz.isNull($ltxtELem.attr("dob"))) {
            apz.ACOP01.AccountOpening.sParams.CustomerDetails.dateOfBirth = $ltxtELem.attr("dob");
        } else if (!apz.isNull($ltxtELem.attr("yob"))) {
            apz.ACOP01.AccountOpening.sParams.CustomerDetails.dateOfBirth = $ltxtELem.attr("yob");
        }
        apz.ACOP01.AccountOpening.sParams.CustomerDetails.placeOfBirth = $ltxtELem.attr("placeOfBirth");
        apz.ACOP01.AccountOpening.sParams.CustomerDetails.address1 = "";
        apz.ACOP01.AccountOpening.sParams.CustomerDetails.address2 = "";
        apz.ACOP01.AccountOpening.sParams.CustomerDetails.address3 = "";
        if (!apz.isNull($ltxtELem.attr("co"))) {
            apz.ACOP01.AccountOpening.sParams.CustomerDetails.address1 = $ltxtELem.attr("co");
        }
        if (!apz.isNull($ltxtELem.attr("loc"))) {
            apz.ACOP01.AccountOpening.sParams.CustomerDetails.address1 += $ltxtELem.attr("loc");
        }
        if (!apz.isNull($ltxtELem.attr("street"))) {
            apz.ACOP01.AccountOpening.sParams.CustomerDetails.address2 = $ltxtELem.attr("street");
        }
        if (!apz.isNull($ltxtELem.attr("vtc"))) {
            apz.ACOP01.AccountOpening.sParams.CustomerDetails.address3 = $ltxtELem.attr("vtc");
        }
        if (!apz.isNull($ltxtELem.attr("po"))) {
            apz.ACOP01.AccountOpening.sParams.CustomerDetails.address3 += $ltxtELem.attr("po");
        }
        apz.ACOP01.AccountOpening.sParams.CustomerDetails.city = $ltxtELem.attr("dist");
        apz.ACOP01.AccountOpening.sParams.CustomerDetails.state = $ltxtELem.attr("state");
        apz.ACOP01.AccountOpening.sParams.CustomerDetails.zipCode = $ltxtELem.attr("pc");
        apz.ACOP01.AccountOpening.sParams.CustomerDetails.Action = "KYC";
        apz.ACOP01.AccountOpening.fnPopulateAadhar();
        //apz.setElmValue("ACOP01__AadharNo__NationalNo", $ltxtELem.attr("uid"));
        //apz.ACOP01.aadharno.sParams.dateOfBirth = $ltxtELem.attr("dob");
        //apz.plugin.store("QRJSON", JSON.stringify(cusjson));
        // console.log("JSON Data " + JSON.stringify(apz.ACOP01.aadharno.sParams.CustomerDetails));
        // apz.setElmValue("ACOP01__AadharNo__Aadhar", apz.ACOP01.aadharno.sParams.CustomerDetails.aadhar)
