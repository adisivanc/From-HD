    apz.data.scrdata.ACOP01__customer_Req.tbAcopCustomerInfo.passportExpiryDate = (params[0].dateOfExpired == "") ? "" : apz.ACOP01.AccountOpening.fnModifyDate(
        params[0].dateOfExpired);
    apz.data.loadData("customer", "ACOP01");
};
apz.ACOP01.AccountOpening.fnModifyDate = function(date) {
    var datearray = date.split("/");
    var date = ("0" + datearray[0]).slice(-2);
    var month = ("0" + datearray[1]).slice(-2);
    var fulldate = datearray[2] + "-" + month + "-" + date;
    return fulldate;
}
