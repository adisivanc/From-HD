apz.ACOP01.AccountOpening.fnFetchCustomerCB = function(params) {
    debugger;
    if (!params.errors) {
        if (params.res.ACOP01__customer_Req != undefined && (params.res.ACOP01__customer_Req.tbAcopCustomerInfo.submitStatus == "" || params.res.ACOP01__customer_Req
            .tbAcopCustomerInfo.submitStatus == undefined || params.res.ACOP01__customer_Req.tbAcopCustomerInfo.submitStatus == "indraft")) {
            apz.show('ACOP01__AccountOpening__MainRow');
            apz.show('ACOP01__AccountOpening__GeneralInfoRow');
            apz.show('ACOP01__AccountOpening__PassportDetailsRow');
            apz.hide('ACOP01__AccountOpening__otpGeneration');
            $("#ACOP01__customer__i__tbAcopCustomerInfo__firstName").focus();
            apz.ACOP01.AccountOpening.sCache.update = true;
            //apz.ACOP01.AccountOpening.fnFetchCustomer();
        } else if (params.res.ACOP01__customer_Req != undefined && params.res.ACOP01__customer_Req.tbAcopCustomerInfo.submitStatus == "submitted") {
            // $(".scanbtn").attr("disabled","disabled");
            apz.ACOP01.AccountOpening.sCache.submitted = true;
            apz.ACOP01.AccountOpening.sCache.update = true;
            apz.dispMsg({
                message: "You seem to have already submitted an application.Click OK to view or edit the same or Cancel to go back ",
                type: "C",
                callBack: apz.ACOP01.AccountOpening.fnViewApplication
            });
        } else {
            apz.ACOP01.AccountOpening.sCache.update = true;
            apz.hide('ACOP01__AccountOpening__RelationshipType');
            apz.hide('ACOP01__AccountOpening__otpGeneration');
            apz.show('ACOP01__AccountOpening__MainRow');
            apz.show('ACOP01__AccountOpening__GeneralInfoRow');
            apz.show('ACOP01__AccountOpening__PassportDetailsRow');
            $("#ACOP01__customer__i__tbAcopCustomerInfo__firstName").focus();
            // apz.toggleModal({
            //     targetId: "ACOP01__AccountOpening__ConfirmationModal"
            // });
        }
    } else if (params.fromBack != undefined && !params.fromBack) {
        apz.ACOP01.AccountOpening.fnViewApplication({
            "choice": true
        });
    } else {
        apz.dispMsg({
            message: "Do you have an existing Relationship?If yes click ok,if no click cancel.",
            type: "C",
            callBack: apz.ACOP01.AccountOpening.fnConfirmCB
        });
    }
}
apz.ACOP01.AccountOpening.fnViewApplication = function(params) {
    if (params.choice) {
        apz.hide('ACOP01__AccountOpening__otpGeneration');
        apz.show('ACOP01__AccountOpening__MainRow');
        apz.show('ACOP01__AccountOpening__GeneralInfoRow');
        apz.show('ACOP01__AccountOpening__PassportDetailsRow');
        $("#ACOP01__customer__i__tbAcopCustomerInfo__firstName").focus();
        apz.ACOP01.AccountOpening.sCache.submitted = true;
        //$(".savenextbtn").text("Next");
        //apz.ACOP01.AccountOpening.fnFetchCustomer();
    } else {
        var lParams = {
            "appId": "ACOP01",
            "scr": "AccountOpening",
            "div": "csmrbk__Login__Launcher",
        }
        apz.launchApp(lParams);
    }
}
