apz.app.onLoad_AccountOpening = function(params) {
    // apz.mockServer = true;
    $("body").removeClass("loginby");
    $("#ACOP01__AccountOpening__sc_row_19").addClass("accheader");
    if (params.customer) {
        apz.hide("ACOP01__AccountOpening__firstRow");
        apz.data.scrdata.ACOP01__customer_Req.tbAcopCustomerInfo = params.customer;
        apz.ACOP01.AccountOpening.fnFetchCustomerCB({
            errors: !params.update,
            res: {},
            fromBack: false
        });
        apz.data.loadData("customer", "ACOP01");
    }
    apz.hide('ACOP01__AccountOpening__RelationshipType');
    /////uploadDocCode
    var isMobile = /iPhone|iPad|iPod|Android/i.test(navigator.userAgent);
    if (isMobile) {
        apz.show("ACOP01__AccountOpening__CaptureBtn");
    }
    if(apz.deviceGroup == "Web"){
         var scand= $("#ACOP01__AccountOpening__scandoc")
        scand.removeClass("sno");
    }
    else{
         var qr= $("#ACOP01__AccountOpening__qrscan")
        qr.removeClass("sno");
    
    }
    apz.ACOP01.AccountOpening.fnOnChangeUploadType();
    $("#ACOP01__AccountOpening__el_btn_25").hide();
};
