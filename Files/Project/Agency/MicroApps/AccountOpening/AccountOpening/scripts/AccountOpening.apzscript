apz.ACOP01.AccountOpening.fnsmsListener = function() {
    debugger;
    var json = {};
    if (apz.deviceGroup == "Mobile") {
        apz.startLoader();
        json.id = "SMSLIST_ID";
        json.callBack = apz.ACOP01.AccountOpening.startSMSCallback;
        apz.ns.startSMSListener(json);
    }
}
apz.ACOP01.AccountOpening.startSMSCallback = function(params) {
    debugger;
    apz.stopLoader();
    if (params.message) {
        
        var listenerotp = params.message.match(/[0-9]{6}/)[0];
        var generatedotp = apz.ACOP01.AccountOpening.sCache.OTPCode + "";
        if (listenerotp == generatedotp) {
            apz.setElmValue("ACOP01__AccountOpening__otpinput", listenerotp);
        }
    }
    
    apz.ns.stopSMSListener("SMSLIST_ID");
}
