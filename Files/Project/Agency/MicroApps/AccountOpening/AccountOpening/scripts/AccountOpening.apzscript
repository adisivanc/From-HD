apz.ACOP01.AccountOpening.fnSave01 = function(ths) {
    var lCount = 0;
    $("input:required").each(function() {
        if ($(this).val() == "") {
            lCount++;
        }
    })
    if (lCount == 0) {
        var refno = apz.data.scrdata.ACOP01__customer_Req.tbAcopCustomerInfo.referenceNo;
        apz.data.buildData("customer", "ACOP01");
        if (!refno) {
            apz.data.scrdata.ACOP01__customer_Req.tbAcopCustomerInfo.referenceNo = Math.floor(100000 + Math.random() * 900000);
        } else {
            apz.data.scrdata.ACOP01__customer_Req.tbAcopCustomerInfo.referenceNo = refno
        }
        var lServerParams = {
            "ifaceName": "customer_New",
            "buildReq": "N",
            "req": apz.data.scrdata.ACOP01__customer_Req,
            "appId": "ACOP01",
            "paintResp": "N",
            "callBack": apz.ACOP01.AccountOpening.fnStep1CB,
            "callBackObj": ths
        };
        if (apz.ACOP01.AccountOpening.sCache.update) {
            lServerParams.ifaceName = "customer_Modify";
        }
        apz.ACOP01.AccountOpening.sCache.update = true;
        apz.server.callServer(lServerParams);
    } else {
        var msg = {};
        msg.code = "APZ-SAVE-001";
        apz.dispMsg(msg);
    }
}
