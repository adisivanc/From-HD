apz.ACOP01.AccountOpening.fnShiftOTP = function(ths, event) {
    if (event && event.which == 8) {
        $(ths).closest("li").prev().find(":input").focus();
    } else if (event && (event.which < 48 || event.which > 57)) {} else {
        $(ths).closest("li").next().find(":input").focus();
    }
}
