apz.appdtl.PersonalDetails.fnCreateDocumentArray = function() {
    debugger;
    apz.idDocArray = [];
    var obj = apz.idDocArray.push({
        "refNo": apz.refNo,
        "docType": "PhotoSignature",
        "docData": apz.selfieImage,
        "docName": "Upload your photo",
        "fileName": "",
        "docId": +new Date
    });
    var docName;
    if (apz.idType === "Passport") {
        docName = "Passport";
    } else {
        docName = "National ID";
    }
    var idArrray = [{
        "refNo": apz.refNo,
        "docType": "IdentityDocuments",
        "docData": apz.frontsideImage,
        "docName": docName,
        "docId": +new Date + 1,
        "fileName": "Front"
    }, {
        "refNo": apz.refNo,
        "docType": "IdentityDocuments",
        "docData": apz.backsideImage,
        "docName": docName,
        "docId": +new Date + 2,
        "fileName": "Back"
    }];
    apz.idDocArray = apz.idDocArray.concat(idArrray);
    apz.selfieImage = "";
    apz.frontsideImage = "";
    apz.backsideImage = "";
    apz.idType = "";
}
