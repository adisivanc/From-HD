/*------------------------------------*\
    Flaunt JS Navigation
\*------------------------------------*/


.nav {
	position:relative;
	display:inline-block;
	font-size:18px;
	/*font-family: 'latosemibold';*/
	font-family: 'LetterGothicMTStd';
	font-weight:700;
}
.nav-list {
	text-align:left;
	display:block;
}
.nav-item {
	float:left;
	*display:inline;
	zoom:1;
	position:relative;
	z-index:9999;
	font-weight:700;
}
.nav-item a {
	display:block;
	color:#FFF;
	padding:0px 20px;
	height:76px;
	line-height:76px;
	font-weight:700;
	/*background:#7A1E61;*/
	/*border-bottom:1px solid #8B2870;*/
}
.caret { width:8px; height:7px; float:right; background:url(../images/mda.png) no-repeat; position:relative; top:34px; left:8px; display: none; } 

.nav-item > a:hover, .nav-item > a.active, .nav-item:hover, .nav-item:hover > a {
	background:#f2f2f2 url(../images/pattern_1.jpg) no-repeat;
	color:#ce9f38;	
}
.nav-item > a:hover .caret, .nav-item > a.active .caret, .nav-item:hover > a .caret { background:url(../images/mda_mo.png) no-repeat; } 

.nav-item:hover .nav-submenu {
	display:block; 
}
/* Navigation submenu */
.nav-submenu {
	display:none;
	position:absolute;
	left:0px;
	width:260px;
	z-index:999;
	background:#f2f2f2 url(../images/pattern_1.jpg) repeat;
	border:1px solid #bbbbbb;
	box-shadow: 3px 3px 7px #333333;
	border-top:0;
	font-weight:700;
	letter-spacing:-1;
}

.reverse_nav { top:auto; bottom:100%; }

.nav-submenu-item a {
	/*background:#8B2770;*/
	border-bottom:1px dotted #aaaaaa;
	display:block;
	/*padding:15px;*/
	line-height:1;
	height: auto;
	padding:10px 10px;
	float:left;
	width:240px;
	font-weight:700;
}
.nav-submenu-item:last-child a { border-bottom:0; }
.nav-submenu-item a:hover {
	background:url(../images/menu_mo.png) repeat;	

	/*background:#932C77;*/
		
}
/* Mobile navigation */
.nav-mobile {
	display:none; /* Hide from browsers that don't support media queries */
	cursor:pointer;
	position:absolute;
	top:0;
	right:0;
	background:#3D3D3B url(../images/nav.svg) no-repeat center center;
	background-size:18px;
	height:34px;
	width:34px;
	padding:2px 0;
}
/* Mobile navigation, clickable area for revealing <ul> */
.nav-click {
	position:absolute;
	top:0;
	right:0;
	display:none;
	border:1px solid #e6b64e;
	height:41px;
	width:50px;
	cursor:pointer;
	background:#ce9f38;
}
.nav-click i {
	display:block;
	height:41px;
	width:48px;
	background:url(../images/drop.svg) no-repeat center center;
	background-size:13px;
}
.nav-click:hover {
	background-color:#e6b64e;
}
.nav-rotate {
	-webkit-transform:rotate(180deg);
	-moz-transform:rotate(180deg);
	-ms-transform:rotate(180deg);
	-o-transform:rotate(180deg);
	transform:rotate(180deg);
}

