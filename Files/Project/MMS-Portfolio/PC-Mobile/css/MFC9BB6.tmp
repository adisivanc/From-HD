* html img,
* html .png{
position:relative;
behavior: expression((this.runtimeStyle.behavior="none")&&(this.pngSet?this.pngSet=true:(this.nodeName == "IMG" && this.src.toLowerCase().indexOf('.png')>-1?(this.runtimeStyle.backgroundImage = "none",
this.runtimeStyle.filter = "progid:DXImageTransform.Microsoft.AlphaImageLoader(src='" + this.src + "', sizingMethod='image')",
this.src = "images/transparent.gif"):(this.origBg = this.origBg? this.origBg :this.currentStyle.backgroundImage.toString().replace('url("','').replace('")',''),
this.runtimeStyle.filter = "progid:DXImageTransform.Microsoft.AlphaImageLoader(src='" + this.origBg + "', sizingMethod='crop')",
this.runtimeStyle.backgroundImage = "none")),this.pngSet=true)
);
}

html { font-size: 100%; -webkit-text-size-adjust: 100%; -ms-text-size-adjust: 100%; }

html, body, div, span, applet, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
a, abbr, acronym, address, big, cite, code,
del, dfn, em, img, ins, kbd, q, s, samp,
small, strike, strong, sub, sup, tt, var,
b, u, i, center,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td,
article, aside, canvas, details, embed,
figure, figcaption, footer, header, hgroup,
menu, nav, output, ruby, section, summary,
time, mark, audio, video { margin:0; padding:0; }

h1, h2, h3, h4, h5, h6 { font-style:normal; font-weight:normal; }
img { border: 0; -ms-interpolation-mode: bicubic; }
a { text-decoration:none; }


/* ---------------------------------- */

body { margin:0 0 0 0; padding:0 0 0 0; font-family: 'robotoregular'; background-color:#FFFFFF; font-size:100%; color:#000000; }

#header { width:100%; height:136px; float:left; background: url(../images/header_strip.png) repeat-x; border-top:7px solid #34567b; position: absolute; z-index:99;  }
.content { max-width:1200px; width:96%; margin:0 auto; }
#logo { margin:27px 0 0 10px; float:left; }

#header_right { float:right; padding-top:45px; }

#contact { float:left; font-family: 'robotomedium'; font-size:36px; color:#FFFFFF; padding-top:3px; margin-right:30px; } 
#contact span { font-family: 'robotoregular'; font-size:24px; float:left; padding:5px 7px 0 0; }
#header_signupbtn { float:right; cursor:pointer; }


#slide_container { width:100%; float:left; background:url(../images/slide_img2.jpg) no-repeat center top transparent; display: block; -webkit-background-size: cover; -moz-background-size: cover; -o-background-size: cover; -ms-background-size: cover; background-size: cover; height:771px;  }


#slideform_container { width:100%; float:left; padding-top:170px; } 
#slideform_outer { width:480px; float:right; background:#335577; color:#FFFFFF; padding:10px 10px; -webkit-border-radius: 7px; -moz-border-radius: 7px; border-radius: 7px; }

.slidetitle { width:100%; float:left; font-family: 'robotobold'; font-size:29px; color:#FFFFFF; padding-bottom:5px;  }

.txtbox { width:97.5%; float:left; font-family: 'robotoregular'; font-size:18px; color:#2c4a6a; height:45px; line-height:45px; border:1px solid #ffffff; outline: none; padding:0 1%; -webkit-border-radius: 5px; -moz-border-radius: 5px; border-radius: 5px; }
.slide_txtbox_date { background-color:transparent; font-size:18px;  color:#FFFFFF; border:1px solid #FFFFFF; outline: none; background: url(../images/cal_icn.png) no-repeat 97% 15px; }
.listbox { width:100%; font-family: 'robotoregular'; font-size:18px; color:#2c4a6a; padding:8px 0; -webkit-border-radius: 5px; -moz-border-radius: 5px; border-radius: 5px; }

#slide_option { width:100%; float:left; font-size:20px; padding:10px 0 7px 0; }
.slide_row { width:100%; float:left; padding:7px 0; }

.slide_rowleft { width:270px; float:left; }
.slide_rowright { float:right; }

.border_white { border:1px solid #ffffff; }


#slide1_desc { width:100%; float:left; font-family: 'robotolight'; font-size:38px; color:#ffffff; letter-spacing:5.4px; padding:50px 0 0 0;  }
.slide1_desc { width:100%; float:left; }
#slide1_desc1 { width:auto; font-family: 'robotoblack'; float:left; font-size:40px; letter-spacing:9px; border-bottom:1px solid #2c4a6a; }




/* -------- Booking Form -------- */


#bookingfrom_header { width:100%; float:left; background:url(../images/header_image.jpg) no-repeat center top transparent; display: block; -webkit-background-size: cover; -moz-background-size: cover; -o-background-size: cover; -ms-background-size: cover; background-size: cover; height:418px;  }

#bookingfrom_hd_outer { width:100%; float:left; padding-top:150px; font-family: 'robotoblack'; font-size:35px; color:#FFFFFF; } 
.bookingfrm_hd_left { float:left; padding-top:10px; letter-spacing:4px; }
.bookingfrm_hd_right { float:right; padding-top:30px; padding-right:20px; }


.bkhd_desc1 { font-family: 'robotobold'; font-size:35px; }  
.bkhd_desc2 { font-size:35px; }
.bkhd_desc2 span { font-family: 'robotolight'; }


#bookingfrom_content { width:100%; float:left; padding:20px 0;  } 
#bookingfrm_outer { width:97%; float:left; background:#f5f5f5; -webkit-border-radius: 10px; -moz-border-radius: 10px; border-radius: 10px; padding:15px 1.5%; }


#bookingfrm_left { width:80px; float:left; }
#bookingfrm_leftsteps { width:5px; background:#FFF; margin:0 auto; }

#bkstep1 { margin-left:-22.5px;  }
#bkstep2 { margin-left:-22.5px; margin-top:333px; }
#bksteptick { margin-left:-22.5px; margin-top:450px; }


#bookingfrm_right { margin-left:100px; }
#bookingfrm_right_content { width:100%; float:left; padding-top:7px; }

.bookingfrmhd { width:100%; float:left; font-size:24px; color:#34577a; padding-bottom:20px; }
.bookingfrmhd_small { font-size:18px; float:right; }
.star { color:#FF0000; font-size:16px; }


.bookingfrm_row { width:100%; float:left; color:#34577a; line-height:27px; padding-bottom:20px; }
.bookingfrm_row_left { width:40%; float:left; padding-right:5%; }
.bookingfrm_row_right { width:40%; float:left; padding-right:5%; }


#bookingfrm_step2 { padding-top:60px; float:left; position:relative; }





#bookingfrm_steptick { width:100%; float:left; font-size:15px; color:#34577a; padding-top:43px; }


.bookingfrm_step2tbl { width:425px; background:#ececec; font-size:18px; color:#34577a; -webkit-border-radius: 10px; -moz-border-radius: 10px; border-radius: 10px; position: absolute; left:45%; margin-top:80px; }
.bookingfrm_step2tbl tr td { padding:0px 10px; }
.bookingfrm_step2tbl tr td:first-child  { }
.bookingfrm_step2tbl tr td:last-child { font-family: 'robotobold'; }




#row_left1 { width:47%; float:left; padding-right:3%; }
#row_left2 { width:47%; float:left; padding-left:3%; }











