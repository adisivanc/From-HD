        lreq.CardFAT.StartDateTime = new Date(apz.finant.AccountFAT.lStartDt).format('Ymdhms');
        lreq.CardFAT.EndDateTime = new Date(apz.finant.AccountFAT.lEndDt).format('Ymdhms');
