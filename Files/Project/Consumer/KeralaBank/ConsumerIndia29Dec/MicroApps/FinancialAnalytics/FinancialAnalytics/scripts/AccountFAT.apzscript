apz.finant.AccountFAT.fn_FormatAmount = function(amt) {
    var lparams = {};
    lparams.value = amt;
    lparams.mask = apz.numberMask;
    lparams.decimalPoints = 2;
    lparams.decimalSep = ".";
    var formatedAmt = apz.formatNumber(lparams);
    return formatedAmt;
};
